{
  "entryPoints": ["entries", "packages"],
  "entryPointStrategy": "Expand",
  "out": "public/docs",
  "plugin": ["typedoc-theme-hierarchy"],
  "theme": "hierarchy",
  "tsconfig": "./tsconfig.typedoc.json",
  "name": "Fuks blog typedoc",
  "exclude": [
    "**/dist/**/*",
    "**/node_modules/**/*",
    "**/.next/**/*",
    "**/.rollup.cache/**/*"
  ]
}
